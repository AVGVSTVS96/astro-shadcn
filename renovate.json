{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended", ":semanticCommitType(chore)"],
  "prHourlyLimit": 6,
  "packageRules": [
    {
      "groupName": "Prettier",
      "matchPackagePatterns": ["^prettier"],
      "matchUpdateTypes": ["minor", "patch"]
    }
  ]
}
